<div class="wrapper">

  <div class="side-bar">
  <div>
     <%= "#{@user.last_name}さんの情報"%>
   </div>
   <div>
     <%= link_to '戻る', root_path , class: :button%>
   </div>

   <div class='all-list'>
        <p>従業員一覧</p>
    </div>

    <% @users.each do |user| %>
        <div class='user-name'>
          <%= link_to user.last_name + user.first_name, user_path(user.id), method: :get %>
        </div>
    <% end %>
  </div>

  <div class="main">
   <div class="chat">
    <%= render partial: "schedules/schedules" %>
    <%= render partial: "schedules/reports" %>
   </div>
  </div>
  
</div>